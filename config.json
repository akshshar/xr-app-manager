                                "type": "docker",
                                "docker_scratch_folder": "/tmp",
                                "docker_container_name": "ubuntu",
                                "docker_image_name": "ubuntu:latest",
                                "docker_image_url": "http://11.11.11.2:9090/ubuntu.tar",
                                "docker_image_action": "import",
                                "docker_mount_volumes": [{
                                                "netns_mount": {
                                                        "host": "/var/run/netns",
